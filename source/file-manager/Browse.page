function selectAll(select) {
  if (($('#check_0').hasClass('fa-square-o')||select==1) && select!=0) {
    $('i[id^="check_"]').removeClass('fa-square-o').addClass('fa-check-square-o');
    $('input[class="extra"]').prop('disabled',false);
  } else {
    $('i[id^="check_"]').removeClass('fa-check-square-o').addClass('fa-square-o');
    $('input[class="extra"]').prop('disabled',true);
  }
}
function selectOne(id) {
  if ($('#'+id).hasClass('fa-square-o')) {
    $('#'+id).removeClass('fa-square-o').addClass('fa-check-square-o');
  } else {
    $('#'+id).removeClass('fa-check-square-o').addClass('fa-square-o');
  }
  var checked = 0;
  $('i[id^="check_"]').each(function(){if ($(this).prop('id')!='check_0' && $(this).hasClass('fa-check-square-o')) checked++;});
  $('input[class="extra"]').prop('disabled',checked==0);
}
function doAction(action,title,id) {
  var popup = $("#dialogWindow");
  var source = $('#'+id).attr('data')<?if (!$subdir){?>+'/'<?}?>;
  var type = $('#'+id).attr('type');
  var path = source.split('/');
  var name = path.pop()||patch.pop();
  if (path[1] != 'mnt') return;
  var user = /^user0?$/.test(path[2]);
  var root = '/'+path[1]+(user ? '/'+path[2] : '');
  var disk = ['disk[0-9]+','disks','remotes'];
<?foreach ($pools as $pool) echo "disk.push(\"$pool\");\n"?>
  var match = user ? '' : disk.join('|');
  switch (action) {
  case 1: // delete folder
    popup.html($("#templateDeleteFolder").html());
    popup.find('#source').val(source);
    var height = 300;
    break;
  case 2: // rename folder
    popup.html($("#templateRenameFolder").html());
    popup.find('#source').val(name);
    var height = 300;
    break;
  case 3: // copy folder
    popup.html($("#templateCopyFolder").html());
    popup.find('#source').val(source);
    popup.find('#target').attr('data-pickroot',root).attr('data-pickmatch',match).fileTreeAttach();
    var height = 600;
    break;
  case 4: // move folder
    popup.html($("#templateMoveFolder").html());
    popup.find('#source').val(source);
    popup.find('#target').attr('data-pickroot',root).attr('data-pickmatch',match).fileTreeAttach();
    var height = 600;
    break;
  case 5: // delete file
    popup.html($("#templateDeleteFile").html());
    popup.find('#source').val(source);
    var height = 300;
    break;
  case 6: // rename file
    popup.html($("#templateRenameFile").html());
    popup.find('#source').val(name);
    var height = 300;
    break;
  case 7: // copy file
    popup.html($("#templateCopyFile").html());
    popup.find('#source').val(source);
    popup.find('#target').attr('data-pickroot',root).attr('data-pickmatch',match).fileTreeAttach(null,null,function(path){popup.find('#target').val(path+name).change();});
    var height = 600;
    break;
  case 8: // move file
    popup.html($("#templateMoveFile").html());
    popup.find('#source').val(source);
    popup.find('#target').attr('data-pickroot',root).attr('data-pickmatch',match).fileTreeAttach(null,null,function(path){popup.find('#target').val(path+name).change();});
    var height = 600;
    break;
  case 9: // view file - not yet implemented
    break;
  case 10: // download file
    location.href = source;
    break;
  }
  if (action > 8) return; // no further action for view and download file
  var proceed = false;
  popup.dialog({
    title: title,
    resizable: false,
    width: 900,
    height: height,
    modal: true,
    show: {effect:'fade', duration: 250},
    hide: {effect:'fade', duration: 250},
    buttons: {
      "_(Proceed)_": function(){
        if (proceed) return;
        var target = popup.find('#target');
        if (target.length) {
          target = target.val();
          var valid = action==2||action==6 ? /^[^\/]*$/ : /^\/mnt\/.+/;
          if (!target || !valid.test(target)) return;
        } else target = '';
        var box =$(this);
        $('div.text').html("<i class='fa fa-refresh fa-spin'></i>_(Running)_ ...");
        proceed = true;
        var makeAction = function(){
          $.post('/plugins/dynamix.file.manager/include/FileManager.php',{action:action,source:encodeURI(source),target:encodeURI(target)},function(data){
            var text = data.status.split(' ');
            if (text.length==3) text = "_(completed)_: "+text[0]+",&nbsp;&nbsp;_(Speed)_: "+text[1]+",&nbsp;&nbsp;_(ETA)_: "+text[2]; else text = _(text[0]);
            $('div.text').html("<i class='fa fa-refresh fa-spin'></i> _(Status)_: "+text);
            if (data.pid) {
              timers.makeAction = setTimeout(makeAction,1000);
              return;
            } else if (data.status=='done') {
              setTimeout(refresh,1000);
              box.dialog('close');
            }
          });
        };
        makeAction();
      },
      "_(Cancel)_": function(){
        var box = $(this);
        if (proceed) {
          clearTimeout(timers.makeAction);
          $.post('/plugins/dynamix.file.manager/include/FileManager.php',{action:99,source:encodeURI(source)},function(){
            setTimeout(refresh,1000);
            box.dialog('close');
          });
        } else {
          box.dialog('close');
        }
      }
    }
  });
  $('.ui-dialog .ui-dialog-titlebar').addClass('menu');
  $('.ui-dialog .ui-dialog-title').css({'text-align':'center','width':'100%'});
  $('.ui-dialog .ui-dialog-content').css({'padding-top':'15px','vertical-align':'bottom'});
  $('.ui-widget-content').css({'background':'<?=$bgcolor?>'});
  $('.ui-button-text').css({'padding':'0px 5px'});
}
function doActions(action,title) {
  var popup = $("#dialogWindow");
  var source = [];
  var type = [];
  $('i[id^="check_"]').each(function(){
    if ($(this).prop('id')!='check_0' && $(this).hasClass('fa-check-square-o')) {
      source.push($('#'+$(this).prop('id').replace('check','row')).attr('data')<?if(!$subdir){?>+'/'<?}?>);
      type.push($('#'+$(this).prop('id').replace('check','row')).attr('type'));
    }
  });
  var bulk = source.length > 1;
  var path = source[0].split('/');
  var name = path.pop()||path.pop();
  if (path[1] != 'mnt') return;
  var user = /^user0?$/.test(path[2]);
  var root = '/'+path[1]+(user ? '/'+path[2] : '');
  var disk = ['disk[0-9]+'];
<?foreach ($pools as $pool) echo "disk.push(\"$pool\");\n"?>
  var match = user ? '' : disk.join('|');
  switch (action) {
  case 1: // delete object
    popup.html($("#templateDeleteObject").html());
    popup.find('#source').val(bulk ? "_(Objects to delete)_: "+source.length : source[0]);
    var height = 300;
    break;
  case 3: // copy object
    popup.html($("#templateCopyObject").html());
    popup.find('#source').val(bulk ? "_(Objects to copy)_: "+source.length : source[0]);
    popup.find('#target').attr('data-pickroot',root).attr('data-pickmatch',match);
    if (bulk || type[0]=='d') popup.find('#target').attr('data-pickfilter','HIDE_FILES_FILTER').fileTreeAttach(); else popup.find('#target').fileTreeAttach(null,null,function(path){popup.find('#target').val(path+name).change();});
    var height = 600;
    break;
  case 4: // move object
    popup.html($("#templateMoveObject").html());
    popup.find('#source').val(bulk ? "_(Objects to move)_: "+source.length : source[0]);
    popup.find('#target').attr('data-pickroot',root).attr('data-pickmatch',match);
    if (bulk || type[0]=='d') popup.find('#target').attr('data-pickfilter','HIDE_FILES_FILTER').fileTreeAttach(); else popup.find('#target').fileTreeAttach(null,null,function(path){popup.find('#target').val(path+name).change();});
    var height = 600;
    break;
  }
  if (action > 8) return;
  var proceed = false;
  popup.dialog({
    title: title,
    resizable: false,
    width: 900,
    height: height,
    modal: true,
    show: {effect:'fade', duration: 250},
    hide: {effect:'fade', duration: 250},
    buttons: {
      "_(Proceed)_": function(){
        if (proceed) return;
        var target = popup.find('#target');
        if (target.length) {
          target = target.val();
          var valid = /^\/mnt\/.+/;
          if (!target || !valid.test(target)) return;
        } else target = '';
        var box =$(this);
        if (bulk && target.length>0 && target.substr(-1)!='/') target += '/';
        $('div.text').html("<i class='fa fa-refresh fa-spin'></i>_(Running)_ ...");
        proceed = true;
        var makeAction = function(){
          $.post('/plugins/dynamix.file.manager/include/FileManager.php',{action:action,source:encodeURI(source.join('\n')),target:encodeURI(target)},function(data){
            var text = data.status.split(' ');
            if (text.length==3) text = "_(completed)_: "+text[0]+",&nbsp;&nbsp;_(Speed)_: "+text[1]+",&nbsp;&nbsp;_(ETA)_: "+text[2]; else text = _(text[0]);
            $('div.text').html("<i class='fa fa-refresh fa-spin'></i> _(Status)_: "+text);
            if (data.pid) {
              timers.makeAction = setTimeout(makeAction,1000);
              return;
            } else if (data.status=='done') {
              setTimeout(refresh,1000);
              box.dialog('close');
            }
        });
        };
        makeAction();
      },
      "_(Cancel)_": function(){
        var box = $(this);
        if (proceed) {
          clearTimeout(timers.makeAction);
          $.post('/plugins/dynamix.file.manager/include/FileManager.php',{action:99,source:encodeURI(source.join('\n'))},function(){
            setTimeout(refresh,1000);
            box.dialog('close');
          });
        } else {
          box.dialog('close');
        }
      }
    }
  });
  $('.ui-dialog .ui-dialog-titlebar').addClass('menu');
  $('.ui-dialog .ui-dialog-title').css({'text-align':'center','width':'100%'});
  $('.ui-dialog .ui-dialog-content').css({'padding-top':'15px','vertical-align':'bottom'});
  $('.ui-widget-content').css({'background':'<?=$bgcolor?>'});
  $('.ui-button-text').css({'padding':'0px 5px'});
}
$(function(){
  timers.browse = setTimeout(function(){$('div.spinner.fixed').show('slow');},100);
  shortcut.add('F2',function(){$.cookie('expert')==null ? $.cookie('expert','expert',{path:'/'}) : $.removeCookie('expert',{path:'/'}); refresh();});
  $.get('/plugins/dynamix.file.manager/include/Browse.php',{dir:"<?=addslashes(urlencode($dir))?>",path:"<?=$path?>",block:$.cookie('expert')||''},function(data){
    clearTimeout(timers.browse);
    var table = $('table.indexer');
