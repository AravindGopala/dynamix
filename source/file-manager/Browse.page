    context.init({preventDoubleContext:false,left:true,above:false});
    table.html(data);
    table.bind('sortEnd',function(e,t){
      var sort = e.target.config.sortList.toString().split(',');
      $.cookie('col',sort[0],{path:'/'});
      $.cookie('dir',sort[1],{path:'/'});
    });
    $('div.spinner.fixed').hide('slow');
    table.tablesorter({headers:{0:{sorter:false},6:{sorter:false}},sortList:[[col,dir],[1,0]],sortAppend:[[1,0]],textAttribute:'data'});
  });
});
